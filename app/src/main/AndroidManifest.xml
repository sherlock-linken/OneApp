<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.witbit.sherlock.oneapp">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.witbit.sherlock.tabs.TabFragmentActivity"/>
        <activity android:name="com.witbit.sherlock.recyclerview.MyRecyclerViewActivity">
        </activity>
        <activity android:name="com.witbit.sherlock.autopager.AutoViewPagerActivity">
        </activity>
    </application>

</manifest>